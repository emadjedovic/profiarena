<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Zip Code</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td><a href="<%= `/users/${user._id}` %>"><%= user.fullName %></a></td>
      <td><%= user.email %></td>
      <td><%= user.zipCode %></td>
      <!--edit link tag-->
      <td>
        <a href="<%=`/users/${user._id}/edit` %>"> Edit </a>
      </td>
      <!--delete link tag-->
      <td>
        <a href="<%= `users/${user._id}/delete?_method=DELETE` %>"
          onclick="return confirm('Are you sure you want to delete this user?')">
          Delete
        </a>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
