<h2>Talents</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td><a href="<%= `/${user.id}` %>"><%= user.first_name %> <%= user.last_name %></a></td>
      <td><%= user.email %></td>
    </tr>
    <% }); %>
  </tbody>
</table>
